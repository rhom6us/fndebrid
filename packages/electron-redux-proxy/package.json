{
  "name": "electron-redux-proxy",
  "version": "1.0.7",
  "description": "A store proxy for running over IPC",
  "keywords": [
    "electron",
    "ipc",
    "proxy",
    "redux",
    "store",
    "storeehancer"
  ],
  "homepage": "https://github.com/rhom6us/electron-redux-proxy#readme",
  "bugs": {
    "url": "https://github.com/rhom6us/electron-redux-proxy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhom6us/electron-redux-proxy.git"
  },
  "license": "MIT",
  "author": "Thomas Butler",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "module": "src/index",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "test": "jest --config jestconfig.json",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/jest": "^25.2.1",
    "@types/redux": "^3.6.0",
    "@types/webpack": "^4.39.2",
    "jest": "^24.9.0",
    "prettier": "^2.0.4",
    "ts-jest": "^24.1.0",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "electron": "^6.0.12",
    "redux": "^4.0.4"
  }
}
