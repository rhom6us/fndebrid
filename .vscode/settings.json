{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    ".gitignore": true,
    "**/*.csproj.user": true,
    "**/bin/": true,
    "**/obj/": true,
    "**/Properties/Resources.resx": true,
    "yarn.lock": true,
    ".github/FUNDING.yml": true,
    ".github/ISSUE_TEMPLATE": true,
    ".vscode/settings.json": false,
    ".yarn": true,
    "**/LICENSE": true,
    "**/node_modules": true,
    "dist/": true,
    "packages/@fndebrid/webpack/lib": true,
    "*.log.json": true
  },
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "npm.exclude": "packages/**",
  "npm.packageManager": "yarn",
  "files.associations": {
    "tslint.json": "jsonc"
  },
  "search.exclude": {
    "packages/webpack/lib/": true
  },
  "importSorter.sortConfiguration.customOrderingRules.rules": [
    {
      "regex": "^[@]",
      "orderLevel": 0
    }
  ],
  "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.count": 180,
  "explorerExclude.backup": {},
  "typescript.tsdk": "node_modules\\typescript\\lib"
}
